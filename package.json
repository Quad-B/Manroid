{
  "name": "manroid",
  "version": "2.0.0",
  "description": "Android Phone Manager",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --version-string.CompanyName=\"Quad B\" --version-string.FileDescription=\"Android (Phone) Manager\"  --version-string.ProductName=\"Manroid\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boyphongsakorn/Manroid.git"
  },
  "author": "BoyPhongsakorn",
  "license": "MIT",
  "build": {
    "appId": "com.boyphongsakorn.saox",
    "copyright": "copyright Â© 2019 Quad B Software",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        },{"target": "portable"}
      ],"compression": "maximum"
    }
  },
  "bugs": {
    "url": "https://github.com/boyphongsakorn/Manroid/issues"
  },
  "homepage": "https://github.com/boyphongsakorn/Manroid#readme",
